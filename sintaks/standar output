<?php
	// Sintaks PHP

	// Standar Output
	// echo, print : untuk mencetak tulisan/variabel
	// print_r : untuk mencetak string/array
	// var_dump : untuk menampilkan isi variabel, tipe data dan ukurannya

	// echo string
	echo 'Latifah Dewi';
  echo "Latifah Dewi"; 
	echo "jum'at";

	// echo angka
	echo 123;

	// echo boolean
	echo true;

	// print (sama dengan echo)
	print "Latifah Dewi";

	// menampilkan string dengan print_r
	print_r("Latifah Dewi");

	//menampilkan string, tipe data, dan ukurannya dengan var_dump
	var_dump("Latifah Dewi");

?>

